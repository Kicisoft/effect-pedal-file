<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/lined_paper">

    <!--New setup button-->
    <kicisoft.effectpedalfile.view.CustomTextView
        android:id="@+id/newSetupButton"
        custom:customFont="fonts/CFMidnight.ttf"
        style="@style/left_menu_item"
        android:text="NEW SETUP"/>

    <!--Load setup butotn-->
    <kicisoft.effectpedalfile.view.CustomTextView
        android:id="@+id/loadSetupButton"
        custom:customFont="fonts/CFMidnight.ttf"
        style="@style/left_menu_item"
        android:text="LOAD SETUP"/>

    <!--Options button-->
    <kicisoft.effectpedalfile.view.CustomTextView
        android:id="@+id/optionsButton"
        custom:customFont="fonts/CFMidnight.ttf"
        style="@style/left_menu_item"
        android:text="OPTIONS"/>

</LinearLayout>